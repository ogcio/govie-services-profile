{
  "name": "govie-services-profile",
  "version": "0.0.1",
  "type": "module",
  "description": "",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "pnpm -r dev",
    "dev:www": "pnpm --filter profile dev:frontend",
    "dev:api": "pnpm --filter profile-api dev",
    "build": "pnpm -r build",
    "build:www": "pnpm --filter profile build",
    "build:api": "pnpm --filter profile-api build",
    "check:formatting": "pnpm -r check:formatting",
    "fix:formatting": "pnpm -r fix:formatting",
    "check:linting": "pnpm -r check:linting",
    "fix:linting": "pnpm -r fix:linting",
    "db:migrate": "pnpm --filter profile-api db:migrate",
    "db:rollback": "pnpm --filter profile-api db:rollback",
    "test": "pnpm -r test",
    "test:www": "pnpm --filter profile test:frontend",
    "test:api": "pnpm --filter profile-api test",
    "test:local": "pnpm -r test:local",
    "test:api:local": "pnpm --filter profile-api test:local",
    "test:www:local": "pnpm --filter profile test:local",
    "prepare": "husky || true",
    "preinstall": "npx only-allow pnpm"
  },
  "keywords": [],
  "authors": [
    {
      "name": "Samuele Salvatico",
      "email": "samuele.salvatico@nearform.com"
    },
    {
      "name": "Matteo La Cognata",
      "email": "matteo.lacognata@nearform.com"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/cli": "^19.6.1",
    "@types/node": "22.10.5",
    "@vitest/coverage-v8": "^2.1.8",
    "concurrently": "^9.1.2",
    "husky": "^9.1.7",
    "nodemon": "^3.1.9",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "tsc-alias": "^1.8.10",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.15.3+sha512.1f79bc245a66eb0b07c5d4d83131240774642caaa86ef7d0434ab47c0d16f66b04e21e0c086eb61e62c77efc4d7f7ec071afad3796af64892fae66509173893a"
