{
  "name": "@ogcio/profile-api",
  "version": "0.0.1",
  "description": "Profile API",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --watch --import tsx src/index.ts",
    "build": "rm -rf dist && tsc -p tsconfig.prod.json && cp -R src/public dist/public",
    "check:formatting": "biome format",
    "fix:formatting": "biome format --write",
    "check:linting": "biome lint",
    "fix:linting": "biome lint --write",
    "prepare": "husky || true",
    "test": "NODE_OPTIONS='--import tsx' vitest run --reporter junit --reporter default --coverage --outputFile=results.xml",
    "migrate": "node db/create-db.js && db-migrate --migrations-dir db/migrations --config db/database.json up",
    "migrate:down": "db-migrate --migrations-dir db/migrations --config db/database.json down",
    "migrate:create": "db-migrate --migrations-dir db/migrations --config db/database.json create $npm_config_file --sql-file"
  },
  "dependencies": {
    "@fastify/autoload": "^6.0.3",
    "@fastify/env": "^5.0.1",
    "@fastify/postgres": "^6.0.1",
    "@fastify/sensible": "^6.0.2",
    "@fastify/swagger": "^9.4.0",
    "@fastify/multipart": "^9.0.1",
    "@fastify/swagger-ui": "^5.2.0",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@fastify/under-pressure": "^9.0.2",
    "@ogcio/api-auth": "^4.29.1",
    "@ogcio/building-blocks-sdk": "^0.1.14",
    "@ogcio/fastify-error-handler": "^5.1.0",
    "@ogcio/fastify-logging-wrapper": "^5.1.0",
    "@sinclair/typebox": "^0.34.13",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "close-with-grace": "^2.1.0",
    "fastify": "^5.2.0",
    "fastify-plugin": "^5.0.1",
    "fastify-raw-body": "^5.0.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/http-errors": "^2.0.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "commitlint": "^19.6.0",
    "fastify-cli": "^7.1.0",
    "husky": "^9.1.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "authors": [
    {
      "name": "Samuele Salvatico",
      "email": "samuele.salvatico@nearform.com"
    },
    {
      "name": "Matteo La Cognata",
      "email": "matteo.lacognata@nearform.com"
    }
  ]
}
