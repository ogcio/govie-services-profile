meta {
  name: Public Servant Can Get Any Profile
  type: http
  seq: 3
}

get {
  url: {{baseUrl}}/api/v1/profiles/:profileId
  body: none
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

params:path {
  profileId: {{profileId}}
}

assert {
  res.status: eq 200
  res.body.data.email: eq {{citizenEmail}}
}