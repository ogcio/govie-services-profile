meta {
  name: Public Servant Can Select Profiles
  type: http
  seq: 3
}

get {
  url: {{baseUrl}}/api/v1/profiles/select?ids={{profileId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

query {
  ids: {{profileId}}
}

vars:post-response {
  profileId: res.body.data && res.body.data[0] ?  res.body.data[0].id : undefined
}

assert {
  res.status: eq 200
  res.body.data: isDefined
  res.body.data[0]: isDefined
  res.body.data[0].id: isDefined
}