meta {
  name: Public Servant Can Upload a CSV of Profiles
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/api/v1/profiles/import-profiles
  body: multipartForm
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

headers {
  Content-Type: multipart/form-data
}

body:multipart-form {
  file: {{csvFile}}
}

assert {
  res.status: eq 200
  res.body.data.length: eq 2
}
